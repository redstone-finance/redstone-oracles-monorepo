{
  "name": "@redstone-finance/oracles-monorepo",
  "version": "0.0.2",
  "description": "",
  "scripts": {
    "global:turbo": "cd $INIT_CWD && turbo",
    "build:all": "turbo build",
    "typecheck:all": "turbo typecheck",
    "global:separate-build": "cd $INIT_CWD && turbo build",
    "global:build-clean-build": "cd $INIT_CWD && turbo build --force",
    "compile:all": "turbo compile",
    "clean:all": "turbo clean",
    "clean:build": "cd $INIT_CWD && yarn clean && turbo build",
    "lint:all": "turbo lint",
    "lint:all:fix": "turbo lint:fix",
    "lint:configs:base": "find . \\( -name node_modules -o -name .terraform -o -name dist -o -name lambda-dist -o -name 'artifacts*' -o -name typechain-types -o -name .openzeppelin -o -name '*autogenerated*' -o -name proposals \\) -prune -o \\( -name '*.json' -o -name .prettierrc.mjs -o -name '*.yml' -o -name '*.yaml' \\) -print | xargs yarn global:prettier",
    "lint:configs": "yarn lint:configs:base --check",
    "lint:configs:fix": "yarn lint:configs:base --write",
    "lint:markdown": "markdownlint-cli2 \"packages/**/*.md{,x}\" \"!packages/**/node_modules\" \"!packages/**/target\" *.md{,x}",
    "lint:markdown:fix": "yarn lint:markdown --fix",
    "lint:yarn-lock": "lockfile-lint --path yarn.lock --allowed-hosts npm yarn github.com --allowed-schemes \"https:\" \"git+https:\" \"npm:\" \"workspace:\" \"patch:\"",
    "lint:circular-deps": "madge --extensions ts -c --circular .",
    "test:all": "turbo run test  --filter=!@redstone-finance/fuel-connector --filter=!@redstone-finance/solana-connector --filter=!@redstone-finance/sui-connector --filter=!@redstone-finance/lambda-all-relayers-manifests",
    "version:all": "lerna version --no-push --no-git-tag-version",
    "publish:all": "lerna publish from-git",
    "syncpack:fix-mismatches": "syncpack fix-mismatches --types !local",
    "syncpack:list": "syncpack list-mismatches --types !local",
    "global:eslint": "cd $INIT_CWD && eslint --report-unused-disable-directives",
    "global:jest": "cd $INIT_CWD && jest",
    "global:jest:cov": "cd $INIT_CWD && jest --json --coverage --testLocationInResults --outputFile=coverage/report.json",
    "global:prettier": "cd $INIT_CWD && prettier",
    "global:rollup": "cd $INIT_CWD && rollup",
    "global:ts-node": "cd $INIT_CWD && ts-node",
    "global:ecs-env-runner": "yarn global:ts-node --swc packages/ecs-env-runner/src/cli.ts --cwd $INIT_CWD ",
    "global:tsc": "cd $INIT_CWD && tsc",
    "global:lambda-key": "echo $PACKAGE_NAME.$(git log -1 --format=%h --abbrev=8).zip",
    "global:lambda-build": "cd $INIT_CWD && tsc -p tsconfig.build.json && esbuild $INDEX_PATH --bundle --minify --platform=node --target=es2022 --outfile=lambda-dist/index.js ${ESBUILD_EXTRA:-} && cp -R ../../node-remote-config lambda-dist/node-remote-config",
    "global:lambda-clean": "cd $INIT_CWD && yarn global:ts-clean && rm -rf lambda-dist",
    "global:lambda-zip": "cd $INIT_CWD && cd lambda-dist && zip -r build.zip *",
    "global:lambda-pushToBucket": "cd $INIT_CWD && KEY=$(yarn global:lambda-key) && aws s3 cp ./lambda-dist/build.zip s3://$BUCKET_NAME/$KEY --only-show-errors --no-progress && echo \"S3 object: $KEY\"",
    "global:lambda-pushDevIreland": "cd $INIT_CWD && BUCKET_NAME=redstone-lambdas-eu-west-1 yarn global:lambda-pushToBucket",
    "global:lambda-pushProdIreland": "cd $INIT_CWD && BUCKET_NAME=redstone-lambdas-prod-eu-west-1 yarn global:lambda-pushToBucket",
    "global:lambda-pushDev": "cd $INIT_CWD && KEY=$(yarn global:lambda-key) && yarn global:lambda-pushDevIreland && SRC_BUCKET=redstone-lambdas-eu-west-1 BUCKETS='redstone-lambdas-eu-west-2 redstone-lambdas-eu-north-1 redstone-lambdas-ap-northeast-1' KEY=$KEY ../../../scripts/s3_copy_to_all.sh",
    "global:lambda-pushProd": "cd $INIT_CWD && yarn global:lambda-pushProdIreland",
    "global:lambda-pushProdAllRegions": "cd $INIT_CWD && KEY=$(yarn global:lambda-key) && yarn global:lambda-pushProdIreland && SRC_BUCKET=redstone-lambdas-prod-eu-west-1 BUCKETS='redstone-lambdas-prod-eu-central-1 redstone-lambdas-prod-eu-west-2 redstone-lambdas-prod-eu-west-3 redstone-lambdas-prod-ap-northeast-1 redstone-lambdas-prod-ap-northeast-3' KEY=$KEY ../../../scripts/s3_copy_to_all.sh",
    "global:object-pushToBucket": "cd $INIT_CWD && aws s3api put-object --bucket $BUCKET_NAME --key $KEY_NAME --body $FILENAME",
    "global:ts-clean": "cd $INIT_CWD && rm -rf dist coverage \"*.tsbuildinfo\"",
    "global:ts-hardhat-clean": "cd $INIT_CWD && yarn global:ts-clean && rm -rf artifacts cache typechain-types",
    "global:depcheck": "cd $INIT_CWD && depcheck",
    "depcheck:all": "yarn workspaces foreach --all run global:depcheck",
    "generate-paths-to-include-in-public-repo": "yarn global:ts-node scripts/generate-paths-to-include-in-public-repo.ts",
    "validate-paths-to-include-in-public-repo": "yarn global:ts-node scripts/validate-paths-to-include-in-public-repo.ts"
  },
  "keywords": [],
  "author": "hatskier <Suvorovalex1997@gmail.com> (https://hatskier.com/)",
  "license": "MIT",
  "devDependencies": {
    "@jest/types": "^29.5.4",
    "@redstone-finance/eslint-plugin": "workspace:*",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.4",
    "@swc/core": "^1.9.2",
    "@types/jest": "^29.5.4",
    "@types/node": "^22.13.10",
    "depcheck": "^1.4.7",
    "esbuild": "^0.25.12",
    "eslint": "^9.39.1",
    "javascript-obfuscator": "^4.1.0",
    "jest": "^29.7.0",
    "jiti": "^2.5.1",
    "lerna": "^7.3.0",
    "lockfile-lint": "^4.14.1",
    "madge": "^8.0.0",
    "markdownlint-cli2": "^0.18.1",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-solidity": "^2.1.0",
    "redstone-prettier-config": "^1.1.0",
    "rollup": "^3.29.5",
    "rollup-obfuscator": "^4.0.0",
    "syncpack": "^11.2.1",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "turbo": "^2.6.1",
    "typescript": "^5.9.2"
  },
  "overrides": {
    "elliptic": "^6.6.1",
    "node-datachannel": "0.25.0"
  },
  "resolutions": {
    "elliptic": "^6.6.1",
    "node-datachannel": "0.25.0",
    "axios": "^1.13.2"
  },
  "dependenciesMeta": {
    "@redstone-finance/mars-rover": {
      "built": true
    },
    "@redstone-finance/suiangria": {
      "built": true
    },
    "deno-bin": {
      "built": true
    },
    "mongodb-memory-server": {
      "built": true
    },
    "node-datachannel": {
      "built": true
    },
    "puppeteer": {
      "built": true
    },
    "sqlite3": {
      "built": true
    }
  },
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*",
      "packages/avs/*",
      "packages/lambda/*"
    ]
  },
  "packageManager": "yarn@4.11.0"
}
