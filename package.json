{
  "name": "@redstone-finance/oracles-monorepo",
  "version": "0.0.2",
  "description": "",
  "scripts": {
    "global:run-on-workspaces": "yarn workspaces foreach -vv $WORKSPACES_SPEC ${PARALLEL:+--parallel} --include ${INCLUDE:-'**'} --exclude ${EXCLUDE:-'[]'} ${DRY_RUN:+--dry-run} --topological-dev run",
    "global:run-all": "WORKSPACES_SPEC=--all yarn global:run-on-workspaces",
    "global:run-all-parallel": "PARALLEL=true yarn global:run-all",
    "global:run-all-deps": "WORKSPACES_SPEC=\"--recursive --from ${FROM:-'**'}\" yarn global:run-on-workspaces",
    "global:run-all-deps-parallel": "PARALLEL=true yarn global:run-all-deps",
    "global:run-if-exists": "cd $INIT_CWD && yarn workspaces foreach --worktree run",
    "build:all": "yarn global:run-all-deps-parallel build",
    "build-dev:all": "yarn global:run-all-deps-parallel build-dev",
    "global:separate-build": "git clean -fdx --exclude=.env packages && cd $INIT_CWD && yarn global:run-if-exists build-dev",
    "global:build-clean-build": "(cd $INIT_CWD && yarn global:run-if-exists build-dev) && git clean -fdx --exclude=.env packages && cd $INIT_CWD && yarn global:run-if-exists build",
    "compile:all": "yarn global:run-all compile",
    "clean:all": "yarn global:run-all-deps-parallel clean",
    "clean:build": "cd $INIT_CWD && yarn clean && yarn build",
    "lint:all": "yarn global:run-all-parallel lint",
    "lint:all:fix": "yarn global:run-all-parallel lint:fix",
    "lint:configs:base": "find . \\( -name node_modules -o -name .terraform -o -name dist -o -name lambda-dist -o -name 'artifacts*' -o -name typechain-types -o -name .openzeppelin -o -name '*autogenerated*' -o -name proposals \\) -prune -o \\( -name '*.json' -o -name .prettierrc.mjs -o -name '*.yml' -o -name '*.yaml' \\) -print | xargs yarn global:prettier",
    "lint:configs": "yarn lint:configs:base --check",
    "lint:configs:fix": "yarn lint:configs:base --write",
    "lint:markdown": "markdownlint-cli2 \"packages/**/*.md\" \"!packages/**/node_modules\" \"!packages/**/target\"",
    "lint:markdown:fix": "yarn lint:markdown --fix",
    "lint:yarn-lock": "lockfile-lint --path yarn.lock --allowed-hosts npm yarn github.com --allowed-schemes \"https:\" \"git+https:\" \"npm:\" \"workspace:\" \"patch:\"",
    "lint:circular-deps": "madge --extensions ts -c --circular .",
    "test:all": "EXCLUDE='{@redstone-finance/fuel-connector,@redstone-finance/solana-connector,@redstone-finance/sui-connector,@redstone-finance/lambda-all-relayers-manifests}' yarn global:run-all-parallel test",
    "version:all": "lerna version --no-push --no-git-tag-version",
    "publish:all": "lerna publish from-git",
    "syncpack:fix-mismatches": "syncpack fix-mismatches --types !local",
    "syncpack:list": "syncpack list-mismatches --types !local",
    "global:eslint": "cd $INIT_CWD && eslint --report-unused-disable-directives",
    "global:jest": "cd $INIT_CWD && jest",
    "global:jest:cov": "cd $INIT_CWD && jest --json --coverage --testLocationInResults --outputFile=coverage/report.json",
    "global:prettier": "cd $INIT_CWD && prettier",
    "global:rollup": "cd $INIT_CWD && rollup",
    "global:ts-node": "cd $INIT_CWD && ts-node",
    "global:tsc": "cd $INIT_CWD && tsc",
    "global:lambda-build": "cd $INIT_CWD && tsc --build tsconfig.build.json && esbuild $INDEX_PATH --bundle --minify --platform=node --target=es2022 --outfile=lambda-dist/index.js && cp -R ../../node-remote-config lambda-dist/node-remote-config",
    "global:lambda-build-dev": "cd $INIT_CWD && tsc --build tsconfig.json && cp -R ../../node-remote-config dist/node-remote-config",
    "global:lambda-clean": "cd $INIT_CWD && yarn global:ts-clean && rm -rf lambda-dist",
    "global:lambda-zip": "cd $INIT_CWD && cd lambda-dist && zip -r build.zip *",
    "global:lambda-pushToBucket": "cd $INIT_CWD && aws s3api put-object --bucket $BUCKET_NAME --key $PACKAGE_NAME.$(git log -1 --format=%h --abbrev=8).zip --body ./lambda-dist/build.zip  && echo \"S3 object: $PACKAGE_NAME.$(git log -1 --format=%h --abbrev=8).zip\"",
    "global:lambda-pushDevIreland": "cd $INIT_CWD && BUCKET_NAME=redstone-lambdas-eu-west-1 yarn global:lambda-pushToBucket",
    "global:lambda-pushProdFrankfurt": "cd $INIT_CWD && BUCKET_NAME=redstone-lambdas-prod-eu-central-1 yarn global:lambda-pushToBucket",
    "global:lambda-pushProdIreland": "cd $INIT_CWD && BUCKET_NAME=redstone-lambdas-prod-eu-west-1 yarn global:lambda-pushToBucket",
    "global:lambda-pushProdLondon": "cd $INIT_CWD && BUCKET_NAME=redstone-lambdas-prod-eu-west-2 yarn global:lambda-pushToBucket",
    "global:lambda-pushProdParis": "cd $INIT_CWD && BUCKET_NAME=redstone-lambdas-prod-eu-west-3 yarn global:lambda-pushToBucket",
    "global:lambda-pushProdStockholm": "cd $INIT_CWD && BUCKET_NAME=redstone-lambdas-prod-eu-north-1 yarn global:lambda-pushToBucket",
    "global:lambda-pushDevStockholm": "cd $INIT_CWD && BUCKET_NAME=redstone-lambdas-eu-north-1 yarn global:lambda-pushToBucket",
    "global:lambda-pushDev": "cd $INIT_CWD && yarn global:lambda-pushDevIreland",
    "global:lambda-pushProd": "cd $INIT_CWD && yarn global:lambda-pushProdIreland",
    "global:lambda-pushProdAllRegions": "cd $INIT_CWD && yarn global:lambda-pushProdFrankfurt && yarn global:lambda-pushProdIreland && yarn global:lambda-pushProdLondon && yarn global:lambda-pushProdParis && yarn global:lambda-pushProdStockholm",
    "global:object-pushToBucket": "cd $INIT_CWD && aws s3api put-object --bucket $BUCKET_NAME --key $KEY_NAME --body $FILENAME",
    "global:ts-clean": "cd $INIT_CWD && rm -rf dist coverage tsconfig.tsbuildinfo tsconfig.build.tsbuildinfo",
    "global:ts-hardhat-clean": "cd $INIT_CWD && yarn global:ts-clean && rm -rf artifacts cache typechain-types",
    "generate-paths-to-include-in-public-repo": "yarn global:ts-node scripts/generate-paths-to-include-in-public-repo.ts",
    "validate-paths-to-include-in-public-repo": "yarn global:ts-node scripts/validate-paths-to-include-in-public-repo.ts"
  },
  "keywords": [],
  "author": "hatskier <Suvorovalex1997@gmail.com> (https://hatskier.com/)",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@jest/types": "^29.5.4",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.4",
    "@swc/core": "^1.9.2",
    "@types/jest": "^29.5.4",
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "esbuild": "^0.19.9",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-workspaces": "^0.11.0",
    "javascript-obfuscator": "^4.1.0",
    "jest": "^29.7.0",
    "jiti": "^2.5.1",
    "lerna": "^7.3.0",
    "lockfile-lint": "^4.14.1",
    "madge": "^8.0.0",
    "markdownlint-cli2": "^0.18.1",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-solidity": "^2.1.0",
    "redstone-prettier-config": "^1.0.0",
    "rollup": "^3.29.5",
    "rollup-obfuscator": "^4.0.0",
    "syncpack": "^11.2.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1"
  },
  "overrides": {
    "elliptic": "^6.6.1",
    "node-datachannel": "0.25.0"
  },
  "resolutions": {
    "elliptic": "^6.6.1",
    "node-datachannel": "0.25.0"
  },
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*",
      "packages/avs/*",
      "packages/lambda/*"
    ]
  },
  "packageManager": "yarn@4.5.0"
}
