      "deviationCheck": {
        "deviationWithRecentValues": {
          "maxPercent": 5,
          "maxDelayMilliseconds": 300000
        }
      }
    },

    "BTC": {
      "source": ["kaiko", "kaiko-v2"]
    },
    "ETH": {
      "source": ["kaiko", "kaiko-v2"]
    },

    "AAVE": {
      "source": ["kaiko", "kaiko-v2"]
    },

