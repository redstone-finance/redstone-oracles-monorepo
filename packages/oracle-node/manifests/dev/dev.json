  "interval": 10000,
  "priceAggregator": "median",
  "defaultSource": ["binance", "currencycom", "coingecko"],
  "sourceTimeout": 8000,
  "deviationCheck": {
    "deviationWithRecentValues": {
      "maxPercent": 25,
      "maxDelayMilliseconds": 300000
    }
  },
  "httpBroadcasterURLs": ["https://httpbin.org/anything"],
  "enableStreamrBroadcaster": false,
  "enableArweaveBackup": false,
  "evmChainId": 1,
  "tokens": {
    "BTC": {},
    "ETH": {},
    "BNB": {}
  }
}
