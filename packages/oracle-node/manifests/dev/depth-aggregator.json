{
  "interval": 15000,
  "priceAggregator": "median",
  "defaultSource": ["coingecko"],
  "sourceTimeout": 50000,
  "deviationCheck": {
    "deviationWithRecentValues": {
      "maxPercent": 25,
      "maxDelayMilliseconds": 300000
    }
  },
  "tokens": {
    "USDC": {},
    "ETH": {},
    "rETH": {
      "source": [
        "uniswap-v3-ethereum-on-chain-weth-100",
        "uniswap-v3-ethereum-on-chain-weth-500",
        "balancer-ethereum-weth",
        "pancake-swap-on-chain-weth-500"
      ]
    },
    "rETH/ETH": {
      "source": ["non-usd-based"]
    },
    "osETH": {
      "priceAggregator": "depth",
      "source": [
        "balancer-ethereum-weth",
        "uniswap-v3-ethereum-on-chain-usdc-500",
        "curve-reth"
      ]
    },
    "ETHx": {
      "priceAggregator": "depth",
      "source": [
        "curve-weth",
        "curve-wsteth",
        "uniswap-v3-ethereum-on-chain-weth-500"
      ]
    },
    "ETHx/ETH": {
      "source": ["non-usd-based"]
    },
    "SWETH": {
      "priceAggregator": "depth",
      "source": [
        "balancer-ethereum-weth",
        "maverick",
        "uniswap-v3-ethereum-on-chain-weth-500"
      ]
    },
    "weETH": {
      "priceAggregator": "depth",
      "source": [
        "balancer-ethereum-reth",
        "balancer-ethereum-weth",
        "curve-weth",
        "maverick"
      ]
    },
    "weETH/ETH": {
      "source": ["non-usd-based"]
    },
    "ezETH": {
      "priceAggregator": "depth",
      "source": [
        "balancer-ethereum-weth",
        "balancer-ethereum-weeth",
        "curve-weth",
        "uniswap-v3-ethereum-on-chain-weth-100"
      ]
    },
    "ezETH/ETH": {
      "source": ["non-usd-based"]
    },
    "rsETH": {
      "priceAggregator": "depth",
      "source": [
        "balancer-ethereum-ethx",
        "curve-sfrxeth",
        "uniswap-v3-ethereum-on-chain-ethx-3000"
      ]
    },
    "wstETH": {
      "source": [
        "balancer-arbitrum-weth",
        "balancer-ethereum-weth",
        "maverick",
        "uniswap-v3-arbitrum-on-chain-weth-100",
        "uniswap-v3-ethereum-on-chain-weth-100",
        "uniswap-v3-optimism-on-chain-weth-100"
      ]
    },
    "sfrxETH": {
      "source": ["ethereum-evm-fetcher"]
    },
    "frxETH": {
      "source": ["curve-eth", "curve-weth"]
    }
  }
}
