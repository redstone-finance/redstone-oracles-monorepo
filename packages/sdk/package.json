    "@types/lodash": "^4.14.195",
    "axios": "^1.8.4",
    "ethers": "^5.7.2",
    "lodash": "^4.17.21",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "msw": "^2.4.10"
  },
  "overrides": {
    "elliptic": "^6.6.1"
  },
  "resolutions": {
    "elliptic": "^6.6.1"
