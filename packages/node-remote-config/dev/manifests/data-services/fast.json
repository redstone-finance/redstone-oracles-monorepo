{
  "interval": 1000,
  "priceAggregator": "median",
  "defaultSource": ["coingecko"],
  "sourceTimeout": 2000,
  "minValidSourcesPercentage": 1,
  "deviationCheck": {
    "deviationWithRecentValues": {
      "maxPercent": 30,
      "maxDelayMilliseconds": 120000
    }
  },
  "tokens": {
    "BTC": {
      "source": ["bitfinex-usd", "bitstamp-usd", "kaiko-v2", "xt"]
    },
    "ETH": {
      "source": ["bitfinex-usd", "bitstamp-usd", "kaiko-v2", "xt"]
    }
  }
}
