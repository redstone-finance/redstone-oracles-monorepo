{
  "interval": 1000,
  "priceAggregator": "median",
  "defaultSource": ["coingecko"],
  "sourceTimeout": 1000,
  "minValidSourcesPercentage": 30,
  "deviationCheck": {
    "deviationWithRecentValues": {
      "maxPercent": 1000000000,
      "maxDelayMilliseconds": 1000
    }
  },
  "tokens": {
    "TSLA": {
