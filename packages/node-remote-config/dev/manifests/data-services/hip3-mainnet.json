        "alltick-stocks-api-deferred",
        "databento",
        "financialmodelingprep-deferred",
        "polygon-deferred",
        "twelve-data-stocks-deferred"
      ],
      "denomination": "USD",
      "rateFeed": false
    },
    "IS_NY_STOCKS_MARKET_OPEN": {
