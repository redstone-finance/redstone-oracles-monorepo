{
  "interval": 1000,
  "priceAggregator": "median",
  "defaultSource": ["coingecko"],
  "sourceTimeout": 8000,
  "deviationCheck": {
    "deviationWithRecentValues": {
      "maxPercent": 25,
      "maxDelayMilliseconds": 120000
    }
  },
  "tokens": {
    "USDT": {
      "source": ["kraken-usd"],
      "deviationCheck": {
        "deviationWithRecentValues": {
          "maxPercent": 5,
          "maxDelayMilliseconds": 120000
        }
      },
      "denomination": "USD",
      "rateFeed": false
    }
  }
}
