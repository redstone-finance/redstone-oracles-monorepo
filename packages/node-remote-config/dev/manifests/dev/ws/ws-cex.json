{
  "interval": 1000,
  "priceAggregator": "median",
  "defaultSource": ["coingecko"],
  "sourceTimeout": 50000,
  "minValidSourcesPercentage": 10,
  "deviationCheck": {
    "deviationWithRecentValues": {
      "maxPercent": 25,
      "maxDelayMilliseconds": 120000
    }
  },
  "tokens": {
    "TSLA": {
      "source": ["ws-twelve-data", "ws-twelve-data-no-closing-price"],
      "denomination": "USD",
      "rateFeed": false
    },
    "GOOG": {
      "source": ["ws-twelve-data", "ws-twelve-data-no-closing-price"],
      "denomination": "USD",
      "rateFeed": false
    },
    "AAPL": {
      "source": ["ws-twelve-data", "ws-twelve-data-no-closing-price"],
      "denomination": "USD",
      "rateFeed": false
    },
    "META": {
      "source": ["ws-twelve-data", "ws-twelve-data-no-closing-price"],
      "denomination": "USD",
      "rateFeed": false
    },
    "IS_NY_STOCKS_MARKET_OPEN": {
      "source": ["is-ny-stocks-market-open-deferred"],
      "denomination": "other",
      "rateFeed": false
    }
  }
}
