{
  "interval": 1000,
  "priceAggregator": "median",
  "defaultSource": ["coingecko"],
  "sourceTimeout": 50000,
  "minValidSourcesPercentage": 10,
  "deviationCheck": {
    "deviationWithRecentValues": {
      "maxPercent": 25,
      "maxDelayMilliseconds": 120000
    }
  },
  "tokens": {
    "TSLA": {
      "source": ["ws-polygon-massive"],
      "denomination": "USD",
      "rateFeed": false
    },
    "IS_NY_STOCKS_MARKET_OPEN": {
      "source": ["is-ny-stocks-market-open-deferred"],
      "denomination": "other",
      "rateFeed": false
    }
  }
}
