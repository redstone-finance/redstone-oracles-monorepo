{
  "name": "@redstone-finance/eslint-plugin",
  "version": "0.9.0",
  "main": "./redstone-eslint-config.ts",
  "exports": {
    ".": "./redstone-eslint-config.ts",
    "./eslint.config": "./eslint.config.ts"
  },
  "license": "MIT",
  "dependencies": {
    "@eslint/js": "^9.39.1",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@typescript-eslint/utils": "^8.46.4",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-array-func": "^5.1.0",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-workspaces": "^0.11.0",
    "typescript-eslint": "^8.46.4"
  },
  "scripts": {
    "clean": "yarn global:ts-clean",
    "typecheck": "yarn global:tsc --noEmit",
    "typecheck:turbo": "yarn global:turbo typecheck",
    "build": "yarn global:tsc -p tsconfig.build.json",
    "build:turbo": "yarn global:turbo build",
    "lint": "yarn global:eslint .",
    "lint:fix": "yarn global:eslint --fix ."
  },
  "publishConfig": {
    "access": "public"
  }
}
