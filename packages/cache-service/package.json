  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc --build tsconfig.build.json && nest build",
    "run-ts": "ts-node",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint .",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "query-arweave": "node scripts/query-arweave.js",
    "measure-throughput": "node scripts/measure-throughput.js"
  },
  "dependencies": {
    "@bundlr-network/client": "^0.8.4",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/mongoose": "^9.2.2",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^2.1.0",
    "@redstone-finance/protocol": "^0.3.3",
    "@redstone-finance/sdk": "^0.3.3",
    "@redstone-finance/streamr-proxy": "^0.3.3",
    "@redstone-finance/utils": "^0.3.3",
    "arweave": "^1.12.2",
    "cache-manager": "^5.1.4",
    "class-transformer": "^0.5.1",
