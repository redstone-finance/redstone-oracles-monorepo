{
  "name": "@redstone-finance/starknet-connector",
  "version": "0.9.0",
  "description": "A tool to inject RedStone data into Starknet compatible smart contracts",
  "license": "MIT",
  "main": "dist/src/index.js",
  "exports": "./dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "clean": "yarn global:ts-clean",
    "build": "yarn global:tsc -p tsconfig.build.json",
    "build:turbo": "yarn global:turbo build",
    "build-dev": "yarn global:tsc -p tsconfig.json",
    "build-dev:turbo": "yarn global:turbo build-dev",
    "sample-run": "yarn global:ts-node test/sample-run.ts",
    "preversion": "yarn build",
    "pack-package": "yarn build && cd dist && npm pack",
    "lint": "yarn global:eslint .",
    "lint:fix": "yarn global:eslint --fix ."
  },
  "dependencies": {
    "@redstone-finance/sdk": "workspace:*",
    "@redstone-finance/utils": "workspace:*",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "starknet": "^6.6.6",
    "zod": "^3.22.4"
  },
  "overrides": {
    "elliptic": "^6.6.1"
  },
  "resolutions": {
    "elliptic": "^6.6.1"
  },
  "files": [
    "src",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  }
}
